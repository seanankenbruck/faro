{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kafka": {
    "BootstrapServers": "localhost:29092",
    "Topic": "faro-metrics",
    "GroupId": "faro-consumer-group",
    "ConsumerConfig": {
      "AutoOffsetReset": "earliest",
      "EnableAutoCommit": false,
      "MaxPollIntervalMs": 300000,
      "SessionTimeoutMs": 45000
    }
  },
  "ClickHouse": {
    "ConnectionString": "Host=localhost;Port=9000;Database=metrics;Username=metrics_user;Password=metrics_pass_123"
  },
  "Consumer": {
    "BatchSize": 1000,
    "BatchTimeoutSeconds": 10
  }
}
